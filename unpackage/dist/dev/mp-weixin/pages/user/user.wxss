
.user {
	width: 100%;
	height: 100%;
	font-size: 30rpx;
	overflow: hidden;
	background-color: #F5F5F5;
}
.title {
	width: 100%;
	height: 300rpx;
	position: relative;
	background: url(data:image/jpeg;base64,)0 0 no-repeat;
	background-size: cover;
}
.msg {
	width: 100%;
	height: 120rpx;
	line-height: 120rpx;
	text-align: right;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	overflow: hidden;
}
.msg image {
	width: 50rpx;
	height: 50rpx;
	margin: auto;
	margin-right: 30rpx;
}
.user-pic {
	width: 100%;
	height: 120rpx;
	position: relative;
}
.pic-box {
	width: 120rpx;
	height: 120rpx;
	margin: 0rpx 30rpx;
	border: 1px solid #FFFFFF;
	border-radius: 50%;
	background-color: #FFFFFF;
	box-sizing: border-box;
}
.user-pic view {
	float: left;
}
.pic-box image {
	width: 100%;
	height: 100%;
	border-radius: 50%;
}
.user-name {
	width: 200rpx;
	height: 100%;
	line-height: 120rpx;
	color: #F9E8DC;
	text-align: center;
}
.user-vip {
	background-color: #FFD43F;
	width: 300rpx;
	height: 60rpx;
	line-height: 60rpx;
	position: absolute;
	right: 0;
	bottom: 0;
	overflow: hidden;
	border-radius: 30rpx 0rpx 0rpx 30rpx;
}
.user-vip image {
	display: inline-block;
	width: 40rpx;
	height: 40rpx;
	margin-bottom: 10rpx;
	margin-left: 30rpx;
	vertical-align: middle;
}
.user-vip text {
	margin-left: 15rpx;
	display: inline-block;
	font-size: 25rpx;
	color: #E49925;
}
.order {
	width: 100%;
	padding: 20rpx;
	box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
	background-color: #FFFFFF;
	border-bottom: 1px solid #CCCCCC;
}
.order view {
	width: 50%;
}
.my-order {
	font-weight: 700;
}
.all-order {
	text-align: right;
}
.all-order image {
	width: 30rpx;
	height: 30rpx;
	margin-bottom: 5rpx;
	margin-left: 10rpx;
	vertical-align: middle;
}
.sever{
	width: 100%;
	height: 150rpx;
	background-color: #FFFFFF;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack:justify;
	-webkit-justify-content:space-between;
	        justify-content:space-between;
}
.sever-list{
	width: 25%;
	height: 100%;
	text-align: center;
}
.sever-list image{
	width: 50rpx;
	height: 50rpx;
	margin-top: 20rpx;
}
.sever-list text{
	display: block;
}
.adv{
	width: 100%;
	height: 300rpx;
}
.adv image{
	width: 100%;
	height: 300rpx;
}
.set-list view:nth-child(5){
	border: none;
}
.list-box{
	width: 100%;
	padding: 30rpx;
	box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack:justify;
	-webkit-justify-content:space-between ;
	        justify-content:space-between ;
	background-color: #FFFFFF;
	border-bottom: 1px solid #CCCCCC;
}
.list-box view{
	width: 50%;
}
.list-box image{
	width: 30rpx;
	height: 30rpx;
}
.set-pic image{
	margin-right: 30rpx;
}
.set-btn{
	text-align:right;
}
.set{
	width: 100%;
	padding: 30rpx;
	box-sizing: border-box;
	margin-top: 20rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack:justify;
	-webkit-justify-content:space-between ;
	        justify-content:space-between ;
	background-color: #FFFFFF;
	border-bottom: 1px solid #CCCCCC;
}
.set view{
	width: 50%;
}
.set image{
	width: 30rpx;
	height: 30rpx;
}

